{
  "fmt": {
    "exclude": [
      "**/*.js",
      "**/*.mjs",
      "**/*.cjs",
      "node_modules/",
      "dist/",
      "build/"
    ],
    "options": {
      "useTabs": false,
      "lineWidth": 100,
      "indentWidth": 2,
      "singleQuote": true,
      "proseWrap": "preserve"
    }
  }
}